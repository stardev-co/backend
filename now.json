{
  "version": 2,
  "alias": ["api.stardev.co"],
  "regions": ["sfo", "bru"],
  "env": {
    "DB_URI": "@db-uri"
  },
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [
    {
      "src": "(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
